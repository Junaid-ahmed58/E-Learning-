<div class="container">
<table class="table">
  <thead>
    <tr>
      <th scope="col">Rating</th>
      <th scope="col">Comment</th>
    </tr>
  </thead>
  <tbody>
  <% @reviews.each do |review|%>
      <tr>
       <td><%= review.rating %></td>
       <td><%= review.comments %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Add review", new_users_course_review_path %>
</div>
